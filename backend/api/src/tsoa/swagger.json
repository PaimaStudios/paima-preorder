			"CommonItemProps": {
				"properties": {
					"purchased": {
						"type": "number",
						"format": "double",
						"description": "Number of purchased items, filled by the backend"
					},
					"supply": {
						"type": "number",
						"format": "double",
						"description": "Optional supply of the item, if omitted the item is considered to be unlimited"
					},
